{
  "name": "esm-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "volta": {
    "node": "22.9.0"
  },
  "files": [],
  "scripts": {
    "analyze": "ANALYZE=true bun run build",
    "start": "./scripts/start.sh",
    "dev": "vite",
    "dev-sandbox": "vite --port 3000",
    "fast-install": "npm i --prefer-offline --progress=false --no-audit --no-fund",
    "http-dev": "npx --yes http-server --port 3000 --proxy 'http://localhost:3000?' -a localhost -o",
    "lint": "biome check && eslint --cache src",
    "lint-fix": "biome check --write && eslint --cache src",
    "install-react": "npm install --silent --no-save --no-audit --no-fund --progress=false react react-dom",
    "install-react-rc": "npm install --silent --no-save --no-audit --no-fund --progress=false react@rc react-dom@rc",
    "build": "npx react-compiler-healthcheck@latest --yes --src 'src/**/*.{ts,tsx}' && bun run install-react-rc && vite build && ([ -z \"$CI\" ] && bun run install-react || true)",
    "preview": "bun run build && vite preview",
    "install-latest": "npm i --no-fund $(bun --silent ./scripts/install-latest.ts) && npx @biomejs/biome migrate --write && biome clean && bun run postinstall",
    "test": "bun test",
    "test-watch": "bun test --watch",
    "tsc": "tsc",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@eslint/js": "^9.12.0",
    "@happy-dom/global-registrator": "^15.7.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bun": "^1.1.10",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "babel-plugin-react-compiler": "^0.0.0-experimental-734b737-20241003",
    "bun": "1.1.27",
    "eslint": "^9.12.0",
    "eslint-plugin-react-compiler": "^0.0.0-experimental-9a75bad-20241003",
    "install-changed": "^1.1.0",
    "lightningcss": "^1.27.0",
    "patch-package": "^8.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8",
    "vite-plugin-checker": "^0.8.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.2.5",
    "wouter": "^3.3.5"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  }
}
